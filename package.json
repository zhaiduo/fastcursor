{
    "name": "fastcursor",
    "displayName": "Fast Cursor Move",
    "description": "Move cursor quickly by hotkey.",
    "version": "0.1.3",
    "publisher": "zhaiduo",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.fastcursor.up.select",
        "onCommand:extension.fastcursor.down.select",
        "onCommand:extension.fastcursor.left.select",
        "onCommand:extension.fastcursor.right.select",
        "onCommand:extension.fastcursor.start.select",
        "onCommand:extension.fastcursor.end.select",
        "onCommand:extension.fastcursor.first.select",
        "onCommand:extension.fastcursor.last.select",
        "onCommand:extension.fastcursor.up",
        "onCommand:extension.fastcursor.down",
        "onCommand:extension.fastcursor.left",
        "onCommand:extension.fastcursor.right",
        "onCommand:extension.fastcursor.start",
        "onCommand:extension.fastcursor.end",
        "onCommand:extension.fastcursor.first",
        "onCommand:extension.fastcursor.last",
        "onCommand:extension.fastcursor.top",
        "onCommand:extension.fastcursor.vcenter",
        "onCommand:extension.fastcursor.bottom"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.fastcursor.up.select",
                "title": "fastcursor.up"
            },
            {
                "command": "extension.fastcursor.down.select",
                "title": "fastcursor.down"
            },
            {
                "command": "extension.fastcursor.left.select",
                "title": "fastcursor.left"
            },
            {
                "command": "extension.fastcursor.right.select",
                "title": "fastcursor.right"
            },
            {
                "command": "extension.fastcursor.up",
                "title": "fastcursor.up"
            },
            {
                "command": "extension.fastcursor.down",
                "title": "fastcursor.down"
            },
            {
                "command": "extension.fastcursor.left",
                "title": "fastcursor.left"
            },
            {
                "command": "extension.fastcursor.right",
                "title": "fastcursor.right"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
